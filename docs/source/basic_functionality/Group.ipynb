   "source": [
    "import k3d\n",
    "\n",
    "positions=[[0,0,0], [0,1,3], [2,2,1]]\n",
    "\n",
    "group = k3d.points(positions, point_size=0.2, shader='mesh') + \\\n",
    "    k3d.line(positions, shader='mesh', width=0.05)\n",
    "group "
   ]
  },
